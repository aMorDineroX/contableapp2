{
  "name": "contableapp2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run verify-ssl && npm run clean",
    "verify-ssl": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"& { Import-Module ./scripts/Utf8Helpers.psm1 -Force; . './scripts/verify-ssl.ps1' }\"",
    "dev:ssl": "npm run generate-certs && node scripts/dev-https.js",
    "dev": "next dev -p 3001",
    "dev:insecure": "next dev",
    "generate-certs": "node \"./scripts/generate-certificates.js\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"& { Import-Module ./scripts/Utf8Helpers.psm1 -Force; . './clean-cache.ps1' }\"",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "3.5.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.10.0",
    "@mui/material": "6.4.2",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-dialog": "^1.1.5",
    "@tanstack/react-query": "5.66.0",
    "@tremor/react": "^3.13.1",
    "@types/next": "9.0.0",
    "chart.js": "4.4.7",
    "date-fns": "^2.30.0",
    "devcert": "^1.2.2",
    "dotenv": "^16.4.7",
    "framer-motion": "10.18.0",
    "next": "^14.1.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.24.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/chart.js": "2.9.41",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "15.4.3",
    "postcss": "^8.4.32",
    "prettier": "3.4.2",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
  "main": "jest.config.js",
  "directories": {
    "test": "tests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  }
}
